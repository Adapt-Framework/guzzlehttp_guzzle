<?xml version="1.0" encoding="UTF-8"?>
<adapt_framework>
    <bundle>
        <name>guzzlehttp_guzzle</name>
        <label>Guzzle</label>
        <type>extension</type>
        <version>6.2.2</version>
        <namespace>\GuzzleHttp</namespace>
        <version_status>release</version_status>
        <availability>public</availability>
        <load_paths>
            <path>src/</path>
            <path>Handler/</path>
            <path>Exception/</path>
            <path>Cookie/</path>
        </load_paths>
        <depends_on>
            <bundle>
                <name>adapt</name>
                <version>2</version>
            </bundle>
            <bundle>
                <name>guzzlehttp_promises</name>
                <version>1.1</version>
            </bundle>
            <bundle>
                <name>guzzlehttp_psr7</name>
                <version>1.3</version>
            </bundle>
            <bundle>
                <name>vendor</name>
            </bundle>
        </depends_on>
    </bundle>
</adapt_framework>
